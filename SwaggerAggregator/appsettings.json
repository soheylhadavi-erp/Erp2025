{
  "ReverseProxy": {
    "Routes": {
      "generalSwagger": {
        "ClusterId": "generalCluster",
        "Match": { "Path": "/swagger/general/{**catch-all}" }
      },
      "accountingSwagger": {
        "ClusterId": "accountingCluster",
        "Match": { "Path": "/swagger/accounting/{**catch-all}" }
      },
      "general": {
        "ClusterId": "generalCluster",
        "Match": { "Path": "/general/{**catch-all}" }
      },
      "accounting": {
        "ClusterId": "accountingCluster",
        "Match": { "Path": "/accounting/{**catch-all}" }
      }
    },
    "Clusters": {
      "generalCluster": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5141/" }
        }
      },
      "accountingCluster": {
        "Destinations": {
          "d1": { "Address": "http://localhost:5233/" }
        }
      }
    }
  }
}
